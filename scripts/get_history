#!/usr/bin/env python

import sys
from electrum import Interface

try:
    addr = sys.argv[1]
except:
    print "usage: get_history <bitcoin_address>"
    sys.exit(1)

i = Interface({'server':'electrum.be:50001:t'})
i.start()
h = i.synchronous_get([ ('blockchain.address.get_history',[addr]) ])[0]
for item in h:
    print item['tx_hash'], item['height']

