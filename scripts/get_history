#!/usr/bin/env python

import sys
from electrum import Network

try:
    addr = sys.argv[1]
except Exception:
    print "usage: get_history <bitcoin_address>"
    sys.exit(1)

n = Network()
n.start(wait=True)
h = n.synchronous_get([ ('blockchain.address.get_history',[addr]) ])[0]
for item in h:
    print item['tx_hash'], item['height']

